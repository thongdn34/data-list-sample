{
  "name": "backend",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "npx husky install",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint --fix \"{src,apps,libs,test}/**/*.ts\"",
    "clean:prod": "rimraf *dist/*",
    "prebuild": "yarn --silent clean:prod",
    "build": "tsc -p tsconfig.prod.json",
    "start:dev": "nodemon",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --watchAll --verbose"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=6",
    "yarn": ">=1"
  },
  "dependencies": {
    "@apollo/gateway": "2.4.7",
    "@apollo/server": "4.7.1",
    "@apollo/subgraph": "2.4.7",
    "@graphql-tools/utils": "10.0.0",
    "@nestjs/apollo": "11.0.6",
    "@nestjs/axios": "2.0.0",
    "@nestjs/cache-manager": "1.0.0",
    "@nestjs/common": "9.4.2",
    "@nestjs/config": "2.3.2",
    "@nestjs/core": "9.4.2",
    "@nestjs/graphql": "11.0.6",
    "@nestjs/mongoose": "9.2.2",
    "@nestjs/platform-express": "9.4.2",
    "axios": "1.4.0",
    "cache-manager": "5.2.1",
    "cache-manager-redis-yet": "4.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "2.30.0",
    "dotenv": "16.1.3",
    "graphql": "16.6.0",
    "graphql-request": "6.1.0",
    "js-yaml": "4.1.0",
    "mongoose": "7.2.2",
    "morgan": "1.10.0",
    "nice-cloud-config-client": "1.0.1",
    "object-hash": "3.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/typescript": "4.0.0",
    "@graphql-codegen/typescript-graphql-request": "5.0.0",
    "@graphql-codegen/typescript-operations": "4.0.0",
    "@jest/globals": "29.5.0",
    "@nestjs/testing": "9.4.2",
    "@types/cache-manager": "4.0.2",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.2",
    "@types/js-yaml": "4.0.5",
    "@types/morgan": "1.9.4",
    "@types/node": "20.2.5",
    "@types/object-hash": "3.0.2",
    "@types/validator": "13.7.17",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.2",
    "nodemon": "2.0.22",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "ts-jest": "29.1.0",
    "ts-morph": "18.0.0",
    "ts-node": "10.9.1",
    "tslib": "2.5.3",
    "typescript": "5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
